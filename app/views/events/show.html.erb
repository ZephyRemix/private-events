<div>
    <h2><%= @event.name %></h2>
    <p>Date: <%= @event.date %></p>
    <p>Location: <%= @event.location %></p>
    <i>Event creator: <%= @event.creator.id %></i>
    <h2>List of attendees</h2>
    <% @event.attendees.each do |attendee| %>
        <p><%= attendee.id %></p>
    <% end %>

</div>